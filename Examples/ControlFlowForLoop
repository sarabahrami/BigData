for (num <- List (1,2,3)){
  println(num)
}
for (str <- Array("hi","bye","welcom")) {
  println(str)
}
 for (num <- Array.range (0,5)){
   println(num)
 }
for (item <- Set(5,6,7)) {
  println(item)
}

for (num <- Range(0,10)) {
  if(num%2==0){
    println(s"$num is even")
  }else{
    println(s"$num is odd")
  }

  val names = List ("John","Abe","Cindy","Cat")

  for (name <-names) {
    if (name.startsWith("C")){
      println(s"$name starts with C")
    }
  }
